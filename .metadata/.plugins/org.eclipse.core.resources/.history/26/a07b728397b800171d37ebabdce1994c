package paquete;

public class MainMatrix {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int y[][];
		y = new int [10][10];
		int num=0;
		int num2=1;
		
		for(int i=0; i<y.length; i++){
			for(int j=0; j<y.length; j++){
				y[i] = num;
				y[j] = num2;
				num++;
				num2++;
			}
		}
		
		for(int i=0; i< y.length; i++){
			for(int j=0; j<y.length; j++){
				System.out.println("y vale: "+ y[i][j]);
			}
		}
	}

}
