package paquete;

public class MainSwitchCase {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char x='b';
		switch(x){
			case 'a':{
				System.out.println("Estamos en la opc 0");
				break;
			}
			case 'b': {
				System.out.println("Estamos en la opc 1");
				break;
			}
		}
	}

}
